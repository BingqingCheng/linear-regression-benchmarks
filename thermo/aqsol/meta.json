{
 "comment": "Aqueous solubility datasets",
 "datasets": [
	     "aqsol_1144.xyz",
    "aqsol_1762_w_smiles.xyz"
 ],
 "elements": [ "C", "N", "O", "S", "H", "F", "Cl", "Br", "I", "P", "B", "Si" ],
 "has_smiles": true,
 "hypersplit": {
  "method": "random",
  "n_splits": 6,
  "train_fraction": 0.75
 },
 "name": "aqsol",
 "periodic": false,
 "splits": [
  {
   "method": "sequential",
   "repeat_fraction_fct": "lambda s,t,p,f: 8",
   "train_fraction": "[ 0.003*2.**n for n in range(int(-np.log(0.002)/np.log(2.0)+1)) ]"
  }
 ],
 "targets": {
  "log_sol_mol_l": {
   "convert": "",
   "metrics": [
    "rmse",
    "mae",
    "rhop",
    "r2"
   ],
   "per_atom": false,
   "scaling": "non-additive",
   "task": "regression"
  }
 }
}
